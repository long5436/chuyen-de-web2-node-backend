{
  "name": "sequelize",
  "version": "1.0.0",
  "license": "MIT",
  "proxy": "http://localhost:5000",
  "scripts": {
    "server": "nodemon --exec babel-node --extensions \".ts,.js\" ./src/index.ts",
    "scss": "sass --watch --no-source-map src/resources/scss:src/public/css --style compressed",
    "grunt": "grunt",
    "gruntwatch": "grunt watch --force",
    "dev": "concurrently \"npm run server\" \"npm run scss\" \"npm run grunt\" \"npm run gruntwatch\""
  },
  "dependencies": {
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6",
    "express-http-proxy": "^1.6.3",
    "express-minify-html-2": "^1.0.1",
    "http-proxy-middleware": "^2.0.6",
    "mysql2": "^2.3.3",
    "requires-port": "^1.0.0",
    "sequelize": "^6.24.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/node": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.7",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^7.4.0",
    "grunt": "^1.5.3",
    "grunt-cli": "^1.4.3",
    "grunt-contrib-uglify": "^5.2.2",
    "grunt-contrib-watch": "^1.1.0",
    "nodemon": "^2.0.20",
    "sass": "^1.55.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "uglify-js": "^3.17.4"
  }
}
